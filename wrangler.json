{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "transcribe-action-button",
  "main": "worker.ts",
  "compatibility_date": "2025-07-14",
  "assets": { "directory": "./public" },
  "observability": { "logs": { "enabled": true } },
  "routes": [{ "custom_domain": true, "pattern": "action.wilmake.com" }],
  "durable_objects": {
    "bindings": [{ "name": "TranscriptStore", "class_name": "TranscriptStore" }]
  },
  "migrations": [{ "tag": "v1", "new_sqlite_classes": ["TranscriptStore"] }]
}
